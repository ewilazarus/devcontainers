{
    "id": "ewilazarus",
    "version": "0.7.0",
    "name": "Ewilazarus' Dev Container Template",
    "description": "A devcontainer template with pre-installed CLI tools that I use on a daily basis.",
    "documentationURL": "https://github.com/ewilazarus/devcontainers/blob/main/README.md",
    "licenseURL": "https://github.com/ewilazarus/devcontainers/blob/main/LICENSE",
    "options": {
        "imageVariant": {
            "type": "string",
            "description": "Debian slim variant",
            "proposals": [
                "bookworm"
            ],
            "default": "bookworm"
        },
        "projectName": {
            "type": "string",
            "description": "The name of the project",
            "default": "my-project"
        }
    },
    "platforms": [
        "Any"
    ]
}
